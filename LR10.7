#define _CRT_SECURE_NO_WARNINGS
#include <iostream>

int main()
{
    setlocale(LC_ALL, "ru");
    int a, b, c;
    printf("Введите стороны треугольника: ");
    scanf("%d %d %d", &a, &b, &c);
    
    if ((a+b > c) && (a+c > b) && (b+c > a))
    {
        printf("Треугольник существует.\n");
    }
    else
    {
        printf("Треугольник не существует.\n");
    }
}
