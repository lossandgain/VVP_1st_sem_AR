#include <iostream>
#include <ctime>
using namespace std;


int main() {
    setlocale(LC_ALL, "ru");
    const int N = 10;
    int A[N];
    srand(time(0));
    for (int i = 0; i < N; i++) {
        A[i] = -10 + rand() % 30;
        printf("%d\t", A[i]);
    }
    int d = 0, f = 1;
    printf("\n\n");
    for (int i = 0; i < N; i++) {
        if (i % 2 == 0) {
            printf("%d\t", A[d]);
            d++;
        }
        else {
            printf("%d\t", A[N - f]);
            f++;
        }
    }
    return 0;
}
